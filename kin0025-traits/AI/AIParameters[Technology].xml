<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <!-- Wyvern -->
  <AIParameterDatatableElement Name="TechnologyDefinitionDrazariUnit2">
    <AIParameter Name="EmpireMilitaryDefense" Value="4 * $Property(ClassEmpire/ClassResearch:UnlockedTechnologyCount)"  />
    <AIParameter Name="EmpireMilitaryOffense" Value="4 * $Property(ClassEmpire/ClassResearch:UnlockedTechnologyCount)"  />
  </AIParameterDatatableElement>

  <!-- Ancient -->
  <AIParameterDatatableElement Name="TechnologyDefinitionDrazariUnit3">
    <AIParameter Name="EmpireMilitaryDefense" Value="8 * ($Property(ClassEmpire/ClassResearch:UnlockedTechnologyCount) - $Property(ClassEmpire/ClassResearch:EraTechnologyCountPrerequisite))"  />
    <AIParameter Name="EmpireMilitaryOffense" Value="8 * ($Property(ClassEmpire/ClassResearch:UnlockedTechnologyCount) - $Property(ClassEmpire/ClassResearch:EraTechnologyCountPrerequisite))"  />
  </AIParameterDatatableElement>

</Datatable>